                    }
                  ]
                }

    post:
      description: Apply a list of licenses.
      body:
        application/json:
          type: object
          properties:
            licenses:
              type: array
              items:
                schema: license
          example:
            {
              "licenses": [
                {
                  "raw": "eyJhbGciOiJSUzUxMiIsInR5cCI6IkpXVCJ9.eyJlbWFpbCI6Im1pY2hhZWxmQGZpdHp5c2l0ZS5jb20iLCJzdWIiOiJ..."
                },
                {
                  "raw": "eyJhbGciOiJSUzUxMiIsInR5cCI6IkpXVCJ9.eyJlbWFpbCI6Im1pY2hhZWxmQGZpdHp5c2l0ZS5jb20iLCJzdWIiOiJ..."
                }
              ]
            }
      responses:
        201:
          description:
            A JSON object containing the raw licenses that were just applied.
          body:
            application/json:
              type: object
              properties:
                licenses:
                  type: array
                  items:
                    schema: license
              example:
                {
                  "licenses": [
                    {
                      "id": 1,
                      "raw": "eyJhbGciOiJSUzUxMiIsInR5cCI6IkpXVCJ9.eyJlbWFpbCI6Im1pY2hhZWxmQGZpdHp5c2l0ZS5jb20iLCJzdWIiOiJ..."
                    },
                    {
